<% end -%>
<% if @kdcs_real != '' -%>

[realms]
 <%= @realm.upcase -%> = {
  kdc = <%= @kdcs_real %>
  kpasswd_server = <%= @kpasswd_servers_real %>
 }
<% end -%>
